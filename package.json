{
    "name": "tsx-express-jwt",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "node server.js",
        "install": "cd client && npm install",
        "build": "cd client && npm run build",
        "server": "nodemon server.js",
        "client": "npm start --prefix client",
        "dev": "concurrently --kill-others \"npm run server\" \"npm run client\""
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "concurrently": "^7.3.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.0.1",
        "express": "^4.18.1",
        "express-jwt": "^7.7.5",
        "ts-utils-julseb": "^2.1.2",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^6.5.2",
        "morgan": "^1.10.0",
        "nodemailer": "^6.7.8"
    },
    "devDependencies": {
        "nodemon": "^2.0.19"
    }
}
