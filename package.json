{
    "name": "tsx-express-jwt",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "node server.js",
        "build": "cd client && yarn build",
        "install": "cd client && yarn",
        "server": "nodemon --watch './**/*.ts' --exec 'ts-node' server/index.ts",
        "client": "cd client && yarn start",
        "dev": "concurrently --kill-others 'yarn server' 'yarn client'"
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "cloudinary": "^1.32.0",
        "concurrently": "^7.4.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-jwt": "^7.7.5",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^6.6.5",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "multer-storage-cloudinary": "^4.0.0",
        "nodemailer": "^6.8.0",
        "ts-utils-julseb": "^2.2.3"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/cookie-parser": "^1.4.3",
        "@types/cors": "^2.8.12",
        "@types/morgan": "^1.9.3",
        "@types/multer": "^1.4.7",
        "@types/nodemailer": "^6.4.6",
        "nodemon": "^2.0.20",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.8.4"
    }
}
